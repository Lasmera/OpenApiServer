{
    "mockServerHost": "http://localhost:35200",
    "routes": [
        {
            "path": ".*",
            "method": "any",

            "handler":  "pipeline",
            "pipeline": [
                {
                    "handler": "validateRequest"
                },
                {
                    "handler": "delay",
                    "value":  0
                },
                {
                    "handler": "mock"
                },
                {
                    "disable":  true,
                    "handler": "proxy",
                    "host": ""
                },
                {
                    "handler": "validateResponse"
                }
            ]
        }
    ]
}